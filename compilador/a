void Programa():
{}
{
	(((Importacao()|Comentario())*|Execucao())|Execucao())(Comentario())*<EOF>
}
void Importacao():
{}
{
	<INTEIRO
}
void Linhas():
{}
{	
    (Igualdade())*
}

void Expressao():
{}
{
	((<ABREPAR>(Expressao())+<FECHAPAR>)
    | <NOT>Expressao()
	| <NOMEDEVARIAVEL>
	| <VALORDEVARIAVEL>)
	[<OPERADOR>Expressao()]
}